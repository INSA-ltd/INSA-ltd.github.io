{% assign primary_font_url = "https://fonts.googleapis.com/css2?family=Oswald:wght@300;500;600&display=swap" %}
{% assign secondary_font_url = "" %}

<link rel="preconnect" href="https://fonts.googleapis.com/" crossorigin>

<link rel="preload" as="style" href="{{ primary_font_url }}">
<link rel="stylesheet" href="{{ primary_font_url }}" media="print" onload="this.media='all'">
<noscript>
  <link rel="stylesheet" href="{{ primary_font_url }}">
</noscript>

<link rel="preload" as="style" href="{{ secondary_font_url }}">
<link rel="stylesheet" href="{{ secondary_font_url }}" media="print" onload="this.media='all'">
<noscript>
  <link rel="stylesheet" href="{{ secondary_font_url }}">
</noscript>
